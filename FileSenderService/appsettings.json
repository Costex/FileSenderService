{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "PathFormat": "Logs/{Date}.txt"
  },
  "AllowedHosts": "*",

  "FileTranscriptionJob": {
    "FilePath": "./Resources",
    "CronSchedule": "0 0 0 ? * *"
  }
}
